{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/rbur0007/vis_2_test/main/stacked_bar_cut_down.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "location_abrev",
      "type": "nominal",
      "title": "Launch Location"
    },
    "y": {"aggregate": "count"},
    "color": {
      "type": "nominal",
      "field": "Status Mission",
      "scale": {
        "domain": ["Success", "Failure"],
        "range": ["#050875", "#FF0004"]
      },
      "title": "Proportion of Mission Successes and Failures by Launch Location",
      "legend": {"orient": "left"}
    }
  },
  "config": {}
}